{
    "admissionwebhook-ns": 
    {
        "add": 
        [
            {
                "/spec/securityContext/": 1202
            },
            {
                "/spec/securityContext/runAsGroup": 1400
            },
            {
                "/spec/securityContext/runAsGroup": 5000
            },
            {
                "/spec/securityContext/runAsNonRoot": true
            },
            {
                "/spec/securityContext/runAsUser": 1200
            },
            {
                "/spec/containers[+]": 
                {
                    "name": "node-app3",
                    "image": "yulypso/node-app:v0.0.1"
                }
            },
            {
                "/spec/containers[*]/volumeMounts[+]": 
                {
                    "mountPath": "/vol",
                    "name": "aws"
                }
            },
            {
                "/spec/containers/0/volumeMounts[+]": 
                {
                    "mountPath": "/specialvolume",
                    "name": "aws"
                }
            },
            {
                "/spec/volumes[+]":
                {
                    "name": "aws",
                    "awsElasticBlockStore":
                    {
                        "volumeID": "vol-00ef5554ce34943c4"
                    }
                }
            }
        ],
        "remove":
        [
            {
                "path": "/spec/securityContext/runAsUser"
            }
        ],
        "replace":
        [
            {
                "/spec/securityContext/runAsUser": 1200
            }
        ],
        "mandatorydata":
        [
            {
                "path": "/spec/securityContext/runAsUser"
            },
            {
                "path": "/metadata/labels/a4c_nodeid"
            },
            {
                "/metadata/annotations/artemis.site": "prod"
            }
        ],
        "forbiddendata":
        [
            {
                "/spec/securityContext/runAsUser": "0"
            },
            {
                "path": "/spec/volumes[*]/hostPath"
            }
        ]        
    }
}